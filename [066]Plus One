class Solution:  
    def plusOne(self, digits):  
        """ 
        :type digits: List[int] 
        :rtype: List[int] 
        """  
        num=0  
        for i in range(len(digits)):  
            num=num*10+digits[i]  
        num=num+1  
        return([int(i) for i in str(num)]) %%要注意它这里是怎么将整数转化为列表形式的，需要学习  
